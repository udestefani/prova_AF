  <calculationView xsi:type="Calculation:ProjectionView" id="ETA_STUD">
    <descriptions/>
    <viewAttributes>
        <viewAttribute id="BIRTH_DATE_STAMP"/>
        <viewAttribute id="NUMEROORE"/>
        <viewAttribute id="SCHEDULEID"/>
        <viewAttribute id="STUD_CLASSE_ISCRIZIONE"/>
        <viewAttribute id="ENRL_DATE_STAMP"/>
        <viewAttribute id="CANCEL_DATE_STAMP"/>
        <viewAttribute id="STUD_ID"/>
        <viewAttribute id="START_DATE_STAMP"/>
        <viewAttribute id="ANNO_MESE_FORMAT"/>
        <viewAttribute id="FLAG_ISCRIZIONE"/>
        <viewAttribute id="FLAG_ANNULLAMENTO"/>
        <viewAttribute id="CALMONTH_ISCR"/>
      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="INTEGER" id="AGE_STUD_DAYS" expressionLanguage="SQL">
          <formula>days_between(&quot;BIRTH_DATE_STAMP&quot;,&quot;START_DATE_STAMP&quot; )</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="AGE_STUD" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;AGE_STUD_DAYS&quot;/365</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="FASCE_ETA_STUD" length="20" expressionLanguage="SQL">
          <formula> CASE
 
 WHEN &quot;AGE_STUD&quot;&lt;=18 THEN 'A18'
 WHEN &quot;AGE_STUD&quot;&gt;=19 AND &quot;AGE_STUD&quot;&lt;=29 then 'A1929'
 WHEN &quot;AGE_STUD&quot;&gt;=30 AND &quot;AGE_STUD&quot;&lt;=39 then 'A3039'
 WHEN &quot;AGE_STUD&quot;&gt;=40 AND &quot;AGE_STUD&quot;&lt;=49 then 'A4049'
 WHEN &quot;AGE_STUD&quot;&gt;=50 AND &quot;AGE_STUD&quot;&lt;=59 then 'A5059'
 WHEN &quot;AGE_STUD&quot;&gt;=60 THEN 'A60'
 ELSE '0'
 
 END</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="Join_5"/>
    </calculationView>
