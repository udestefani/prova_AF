        <viewAttribute id="FASCE_ETA_STUD"/>
        <viewAttribute id="Rank_Column"/>
        <viewAttribute id="CPNT_ID"/>
        <viewAttribute id="CPNT_TYP_ID"/>
        <viewAttribute id="ENRL_DATE_STAMP"/>
        <viewAttribute id="STUD_CANCEL_DATE"/>
        <viewAttribute id="STUD_ID"/>
        <viewAttribute id="START_DATE_STAMP"/>
        <viewAttribute id="ANNO_MESE"/>
        <viewAttribute id="COUNTER_ENROLLED_ISCR"/>
        <viewAttribute id="FLAG_ANNULLAMENTO"/>
        <viewAttribute id="SUBJ_ID"/>
        <viewAttribute id="CALMONTH_ISCR"/>
        <viewAttribute id="ID_DOMINIO"/>
        <viewAttribute id="DESC_DOMINIO_SICUREZZA_D"/>
        <viewAttribute id="DESCRIZIONE_SOURCE_I"/>
        <viewAttribute id="DESC_DOMINIO_SICUREZZA_I"/>
        <viewAttribute id="DESCRIZIONE_SOURCE_D"/>
        <viewAttribute id="SOURCE"/>
        <viewAttribute id="ID_DOMINIO_SOURCE"/>
      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="NVARCHAR" id="CMPL_STAT_ID_FORMAT" length="256" expressionLanguage="SQL">
          <formula>case when &quot;CMPL_STAT_ID&quot; is null then 'COURSE_NotComplete' else &quot;CMPL_STAT_ID&quot; end</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="KEY_DIM_CORSO_ED" length="256" expressionLanguage="SQL">
          <formula>&quot;CPNT_ID&quot; || COALESCE(&quot;CPNT_TYP_ID&quot;,'')|| COALESCE(&quot;SCHEDULEID&quot;,'') </formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="COUNTER_COMPLETED_STUD" expressionLanguage="SQL">
          <formula>CASE WHEN &quot;CMPL_STAT_ID_FORMAT&quot; IN('CERTIFICATION_Complete', 'COURSE_Complete', 'HALBTAGSVERANST_Complete', 'KURSFOLGE_Complete', 'LEHRGANG_Complete', 'ONLINE_Complete', 'SEMINARREIHE_Complete', 'SEMINAR_Complete', 'WERKSTAETTE_Complete')  THEN 1
ELSE 0

END</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="FASCIA_STUD_ONLINE" length="127" expressionLanguage="SQL">
          <formula>CASE
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A18' THEN 'A18'
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A1929' THEN 'A1929'
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A3039' THEN 'A3039'
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A4049' THEN 'A4049'
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A5059' THEN 'A5059'
WHEN &quot;LABEL_VALUE&quot;='Online' AND &quot;FASCE_ETA_STUD&quot; = 'A60' THEN 'A60'
END</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DATE" id="ANNO_MESE_CALC" expressionLanguage="SQL">
          <formula>CASE WHEN (&quot;CALMONTH_ISCR&quot; is null or &quot;CALMONTH_ISCR&quot; = '') then &quot;ANNO_MESE&quot; else &quot;CALMONTH_ISCR&quot; end</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="Rank_Stud">
