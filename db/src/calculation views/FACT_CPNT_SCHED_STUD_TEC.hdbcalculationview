        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="STUD_CLASSE_ISCRIZIONE" length="10" expressionLanguage="SQL">
          <formula>CASE

WHEN &quot;YEAR_ISCR&quot;&lt;1 THEN '&lt;1 year'
WHEN &quot;YEAR_ISCR&quot;&gt;=1 AND &quot;YEAR_ISCR&quot;&lt;5 THEN '1-5 years'
WHEN &quot;YEAR_ISCR&quot;&gt;=5 THEN '&gt;5 years'

END</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
